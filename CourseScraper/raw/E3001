<script>
(function(){
    // Check if international student (see also global.js:setStudentTypeFromParam() )
    var international_only = window.location.search.indexOf("international=true") !== -1;

    // Check if we arrived from a shortlink (todo: refactor linkref and referrer into a map function)
    var linkref = "";
    if (window.sessionStorage['linkref']) {
        linkref = window.sessionStorage['linkref'];
        window.sessionStorage.removeItem('linkref');
    }

    // Check for a referrer
    var referrer = "";
    if (window.sessionStorage['referrer']) {
        referrer = window.sessionStorage['referrer'];
        window.sessionStorage.removeItem('referrer');
    }

    // Push course data to GTM
    window.dataLayer = window.dataLayer || [];
    window.dataLayer.push({
        "pageType" : "Course details",
        "courseTitle" : "Engineering",
        "courseCode" : "E3001",
        "courseYear" : "2017",
        "courseInterestAreas" : ["Engineering"],
        "courseLevel" : "Undergraduate single degrees",
        "courseQualification" : "Bachelor degree (honours)",
        "courseLocations" :  ( international_only ? 
            ["IN CLAYTON FT"," IN MALAYSIA FT"] :
            ["IN CLAYTON FT PT"," IN MALAYSIA FT PT"] 
        ),
        "courseDurationFullTime" : "4",
        "courseCommenceInMonths" : ( international_only ?
            ["February"," July"] :
            ["February"," July"]
        ),
        "studentType" : ( international_only ?
            "international" :
            "domestic"
        ),
        "linkref": linkref,
        "referrer": referrer
    });
})();
</script>
