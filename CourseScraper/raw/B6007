                    <a href="#overview-1" class="tabs__link js-tabs__link--w-nested">Overview</a>
                </li>
                                                <li class="tabs__item">
                    <a href="#entry-requirements-2" class="tabs__link">Entry requirements</a>
                </li>
                                                <li class="tabs__item">
                    <a href="#course-structure-3" class="tabs__link">Course structure</a>
                </li>
                                                                                </ul>
            <!-- /Tabs Heading -->
            <!-- Tabs Content -->
            <div class="tabs__tab-content">
        <!-- Overview Tab -->
        <div class="tabs__target" id="overview-1" data-name="Overview">
        <div id="overview-tab-content" class="slider-tab-content">
            <!-- International Only -->
                        <!-- /International Only -->
            <!-- Overview Introduction -->
            <p></p>
            <!-- /Overview Introduction -->
            <!-- If Double -->
                            <!--  Specialist Course-->
                                    <!-- Course Specialisations -->
                                            <!-- Specialisations [Sort by name] -->
                        <p id="specialisations" class="introduction">Please select a specialisation for more details:</p>
<ul class="btn-boxes row">
<li class="btn-box col-xs-6 col-md-3">
    <a class="slideRightBtn btn-box--valign" id=".s429141" class="slideRightBtn btn-box--valign" href="https://www.study.monash/_courses/all/2017/specialisations/d/diplomacy-and-trade-xs0104?SQ_DESIGN_NAME=blank&SQ_PAINT_LAYOUT_NAME=specialisation&domestic=true" >
        Diplomacy and trade
        
            
        
